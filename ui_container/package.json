  "scripts": {
    "clean": "rm -rf dist",
    "build": "npm run build:sass && npm run build:client && npm run build:server",
    "start": "node dist/server.js",
    "build:client": "webpack --progress --colors --config webpack.config.js",
    "build:server": "webpack --progress --colors --config webpack.config.server.js",
    "build:sass": "node-sass ./scss/main.scss ./dist/static/next.app.css",
    "watch": "concurrently --kill-others 'webpack --progress --colors --config webpack.config.server.js --watch' 'webpack-dev-server --config webpack.config-js-watch.js --port 8555 --host 0.0.0.0' 'NODE_ENV=dev node dist/server.js'"
  },
